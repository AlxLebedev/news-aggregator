{
  "name": "news-aggregator",
  "version": "1.0.0",
  "description": "using news-api",
  "main": "index.js",
  "scripts": {
    "start": "cross-env  NODE_ENV=development webpack-dev-server --mode development --open --watch",
    "build": "cross-env  NODE_ENV=production rimraf dist && webpack --mode production",
    "show:dist": "live-server dist",
    "lint": "eslint .",
    "stylelint": "stylelint \"src/scss/**/*.scss\" --syntax scss",
    "stylelint:fix": "stylelint \"src/scss/**/*.scss\" --syntax scss --fix",
    "doc": "jsdoc -c jsdoc.config.json --readme ./README.md"
  },
  "browserslist": [
    "Chrome >= 49",
    "ChromeAndroid >= 49",
    "Firefox >= 68",
    "FirefoxAndroid >= 68",
    "Safari >= 9",
    "iOS >= 9",
    "Opera >= 36",
    "OperaMobile >= 36",
    "Samsung >= 5",
    "IE 11",
    "Edge >= 12"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlxLebedev/news-aggregator.git"
  },
  "author": "Alexey Lebedev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlxLebedev/news-aggregator/issues"
  },
  "homepage": "https://github.com/AlxLebedev/news-aggregator#readme",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "jsdoc": "^3.6.5",
    "live-server": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/core": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "autoprefixer": "^9.8.4",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^9.0.1",
    "style-loader": "^1.2.1",
    "svg-sprite-loader": "^5.0.0",
    "svg-spritemap-webpack-plugin": "^3.5.7",
    "svgo-loader": "^2.2.1",
    "swiper": "^6.0.2",
    "terser-webpack-plugin": "^3.0.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-md5-hash": "0.0.6",
    "webpack-merge": "^5.0.8"
  }
}
